//Deque in C++ STL

#include<bits/stdc++.h>
using namespace std;
int main()
{
    deque<int> dq={10, 20, 30};
    dq.push_front(5);
    dq.push_back(40);
    for(auto x:dq)
        cout<<x<<" ";
    cout<<endl;
    auto it=dq.begin();
    it++;
    dq.insert(it, 8);
    dq.pop_front();
    dq.pop_back();
    for(auto x:dq)
        cout<<x<<" ";
    cout<<endl;
    cout<<dq.size();
    return 0;
}
